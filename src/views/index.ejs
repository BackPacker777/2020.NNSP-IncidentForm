            <div id="transport">
                <%- include ('public/views/transport'); -%>
            </div>
        </fieldset>
        <br>

        <fieldset>
            <div class="grid-x grid-padding-x expander">
                <div class="small-9 cell vertical-center"><h6><strong>Witnesses</strong></h6></div>
                <div class="small-2 cell vertical-center"></div>
                <div class="small-1 cell vertical-center" id="wtSymbol"></div>
            </div>
            <div id="witnesses">
                <%- include ('public/views/witnesses'); -%>
            </div>
        </fieldset>
        <br>

        <fieldset>
            <div class="grid-x grid-padding-x expander">
                <div class="small-9 cell vertical-center"><h6><strong>Report Completer</strong></h6></div>
                <div class="small-2 cell vertical-center"></div>
                <div class="small-1 cell vertical-center" id="rcSymbol"></div>
            </div>
            <div id="completer">
                <%- include ('public/views/completer'); -%>
