<div class="grid-container fluid" id="mainPage">
	<!--<form id="mainForm" action="javascript:0">-->
    <form id="mainForm">
        <fieldset>
            <div class="grid-x grid-padding-x expander">
                <div class="small-9 cell vertical-center"><h6><strong>Date &amp; Time</strong></h6></div>
                <div class="small-2 cell vertical-center"></div>
                <div class="small-1 cell vertical-center" id="dtSymbol"></div>
            </div>
            <div id="dateTime">
                <%- include ('public/views/dateTime'); -%>
            </div>
        </fieldset>
        <br>

        <fieldset>
            <div class="grid-x grid-padding-x expander">
                <div class="small-9 cell vertical-center"><h6><strong>Injured Person</strong></h6></div>
                <div class="small-2 cell vertical-center"></div>
                <div class="small-1 cell vertical-center" id="ipSymbol"></div>
            </div>
            <div id="injuredPerson">
                <%- include ('public/views/injuredPerson'); -%>
            </div>
        </fieldset>
        <br>

        <fieldset>
            <div class="grid-x grid-padding-x expander">
                <div class="small-9 cell vertical-center"><h6><strong>Patient History</strong></h6></div>
                <div class="small-2 cell vertical-center"></div>
                <div class="small-1 cell vertical-center" id="phSymbol"></div>
            </div>
            <div id="patientHistory">
                <%- include ('public/views/patient_history'); -%>
            </div>
        </fieldset>
        <br>

        <fieldset>
            <div class="grid-x grid-padding-x expander">
                <div class="small-9 cell vertical-center"><h6><strong>Location</strong></h6></div>
                <div class="small-2 cell vertical-center"></div>
                <div class="small-1 cell vertical-center" id="loSymbol"></div>
            </div>
            <div id="location">
                <%- include ('public/views/location'); -%>
            </div>
        </fieldset>
        <br>

        <fieldset>
            <div class="grid-x grid-padding-x expander">
                <div class="small-9 cell vertical-center"><h6><strong>Skiing History</strong></h6></div>
                <div class="small-2 cell vertical-center"></div>
                <div class="small-1 cell vertical-center" id="shSymbol"></div>
            </div>
            <div id="skiingHistory">
                <%- include ('public/views/skiingHistory'); -%>
            </div>
